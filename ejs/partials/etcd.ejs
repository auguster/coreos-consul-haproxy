  etcd2:
    name: <%= host ? host : 'host' %>
    discovery-srv: <%= domain %>
    initial-cluster-state: <% if (!bootstrap) { -%>   existing<% } else { -%>new<% } %>
    initial-advertise-peer-urls: http://<%= host %><%= ddomain %>:2380
    advertise-client-urls: http://<%= host %><%= ddomain %>":2379
    listen-client-urls: "http://0.0.0.0:2379,http://0.0.0.0:4001"
    listen-peer-urls: http://0.0.0.0:2380
    heartbeat-interval: 500
    election-timeout: 5000